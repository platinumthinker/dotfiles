Config { font = "xft:DejaVu Sans:size=8"
       , bgColor = "#221818"
       , fgColor = "#dedede"
       , position = Top
       , commands = [ Run Cpu ["-L","3","-H","50","--high","#f0c040"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
					, Run Date "%a %b %_d  %H:%M" "date" 60
                    , Run StdinReader ]
, sepChar = "%"
, alignSep = "}{"
, template = "%StdinReader% }{ %cpu% %memory% %date%" }
