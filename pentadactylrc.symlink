colorscheme solarized-dark
set defsearch=duckduckgo
set verbose=0
set guioptions=bCrsn
set titlestring=
set activate=addons,bookmarks,diverted,downloads,extoptions,help,quickmark,tabopen,paste
set complete=slfS

autocmd LocationChange commafeed.com :normal! <C-z>
