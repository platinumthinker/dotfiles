! Swap left Alt and left Control
clear Lock
clear control
clear mod1

keycode 0x25 = Alt_L
keycode 0x6c = Alt_R

keycode 0x40 = Control_L
add control = Control_L
add mod1 = Alt_L Alt_R

! CapsLock now acts as Ctrl
keycode 0x42 = Control_L
add control = Control_L

! Right Control as Menu (context menu)
keycode 0x69 = Menu NoSymbol Menu
